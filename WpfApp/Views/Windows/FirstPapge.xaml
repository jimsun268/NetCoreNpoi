<UserControl x:Class="WpfApp.Views.Windows.FirstPapge"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp.Views.Windows"
             mc:Ignorable="d" 
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"                
             xmlns:prism="http://prismlibrary.com/"            
             prism:ViewModelLocator.AutoWireViewModel="True"
             d:DesignHeight="450" d:DesignWidth="800" >
   
    <Grid>        
        <StackPanel >
            <Button Command="{Binding ButtonCommand}" CommandParameter="1" Content="ModelExcelExport" Height="200" Margin="0,10,0,0"/>
            <Button Command="{Binding ButtonCommand}" CommandParameter="2" Content="ModelExcelFontColor" Height="200" Margin="0,10,0,0"/>
            <Button Command="{Binding ButtonCommand}" CommandParameter="3" Content="3" Height="200" Margin="0,10,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
